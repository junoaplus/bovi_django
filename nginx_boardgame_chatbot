server {
    listen 80;
    server_name your-ec2-ip your-domain.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    
    location /static/ {
        root /home/ubuntu/boardgame_chatbot;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/boardgame_chatbot/boardgame_chatbot.sock;
    }
}
